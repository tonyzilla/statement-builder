{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": false,
      "allowSyntheticDefaultImports": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "Components/*": ["src/components/*"],
        "Hooks/*": ["src/hooks/*"],
        "Store/*": ["src/store/*"]
      }
    }
  }